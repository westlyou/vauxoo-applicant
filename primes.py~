class prime_class(object):
    """
    Class to 
    """
    def is_prime(num):
        x = 0
        for i in range(1, num + 1):
            if (num % i == 0):
                x = x + 1
        if (x == 2):
            print False
        else:
            print True
        return True
    
    num = raw_input('Enter a number ')
    try:
        num = int(num)
        is_prime(num)
    except ValueError:
        print 'The value is not a number'
        pass

    
    
    
